{
  "name": "qi-launch",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "biome check --reporter summary .",
    "lint:fix": "biome check --fix .",
    "format": "biome format --write .",
    "tauri": "tauri",
    "tauri:build": "tauri build",
    "tauri:build:notify": "tauri build",
    "build:mac:arm64": "tauri build --target aarch64-apple-darwin --config src-tauri/tauri.macos.conf.json",
    "build:mac:intel": "tauri build --target x86_64-apple-darwin --config src-tauri/tauri.macos.conf.json",
    "build:mac:universal": "tauri build --target universal-apple-darwin --config src-tauri/tauri.macos.conf.json",
    "build:mac:all": "tauri build --target aarch64-apple-darwin --config src-tauri/tauri.macos.conf.json && tauri build --target x86_64-apple-darwin --config src-tauri/tauri.macos.conf.json && tauri build --target universal-apple-darwin --config src-tauri/tauri.macos.conf.json"
  },
  "dependencies": {
    "@ant-design/icons": "~6.1.0",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-opener": "^2.5.3",
    "@tauri-apps/plugin-os": "^2.3.2",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@tauri-apps/plugin-store": "~2.4.2",
    "antd": "^6.1.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-slick": "^0.31.0",
    "slick-carousel": "^1.8.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@tauri-apps/cli": "^2.9.6",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/react-slick": "^0.23.13",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1"
  }
}
